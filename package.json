  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.3.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "9.39",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "pixi-filters": "^6.1.5",
    "pixi.js": "^8.16.0"
  }
}
